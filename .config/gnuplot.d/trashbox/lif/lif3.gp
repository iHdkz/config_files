#

_term0 = _y0 / (_x0 - _x1) / (_x0 - _x2) / (_x0 - _x3);
_term1 = _y1 / (_x1 - _x0) / (_x1 - _x2) / (_x1 - _x3);
_term2 = _y2 / (_x2 - _x0) / (_x2 - _x1) / (_x2 - _x3);
_term3 = _y3 / (_x3 - _x0) / (_x3 - _x1) / (_x3 - _x2);

lif3(x) = _term0 * (x - _x1)*(x - _x2)*(x - _x3) \
	+ _term1 * (x - _x0)*(x - _x2)*(x - _x3) \
	+ _term2 * (x - _x0)*(x - _x1)*(x - _x3) \
	+ _term3 * (x - _x0)*(x - _x1)*(x - _x2);
