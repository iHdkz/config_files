#

load "nif3.gp";

_F4 =  _y0/((_x0 - _x1)*(_x0 - _x2)*(_x0 - _x3)*(_x0 - _x4))\
     + _y1/((_x1 - _x0)*(_x1 - _x2)*(_x1 - _x3)*(_x1 - _x4))\
     + _y2/((_x2 - _x0)*(_x2 - _x1)*(_x2 - _x3)*(_x2 - _x4))\
     + _y3/((_x3 - _x0)*(_x3 - _x1)*(_x3 - _x2)*(_x3 - _x4))\
     + _y4/((_x4 - _x0)*(_x4 - _x1)*(_x4 - _x2)*(_x4 - _x3));

nif4(x) = nif3(x) + _F4 * (x - _x0)*(x - _x1)*(x - _x2)*(x - _x3);
